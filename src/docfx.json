{
    "metadata": [
      {
        "src": [
          {
            "files": [ "**.csproj" ],
            "exclude": [ "**/bin/**", "**/obj/**" ],
            "src": "./submodules/runtime/src"
          }
        ],
        "dest": "obj/temp/docfxapi"
      }
    ],
    "build": {
      "content": [
        {
          "files": [ "**/*.yml" ],
          "src": "obj/temp/docfxapi",
          "dest": "api"
        },
        {
          "files": "RELEASENOTE.md",
          "src": ".."
        },
        {
          "files": [ "tutorial/**/*.{md,yml}", "spec/**/*.{md,yml}", "*.{md,yml}", "guideline/**/*.{md,yml}", "templates-and-plugins/**/*.{md,yml}" ]
        }
      ],
      "resource": [
        {
          "files": [ "**/images/**", "**/artifacts/**", "codesnippet/**", "langwordmapping/**" ],
          "exclude": [ "_site/**", "**/obj/**", "**.meta" ]
        },
        {
          "files": "schemas/**schema.json",
          "src": ".."
        }
      ],
      "overwrite": "apispec/*.md",
      "xrefService": [
        "https://xref.docs.microsoft.com/query?uid={uid}"
      ],
      "postProcessors": [ "ExtractSearchIndex" ],
      "globalMetadata": {
        "_appTitle": "企业私有化 SaaS 服务 - API 说明",
        "_appFooter": "<span>Copyright © Wang Yucai.<br>Generated by <strong>DocFX</strong></span>",
        "_gitContribute": {
          "branch": "dev"
        }
      },
      "fileMetadata": {
        "priority": {
          "obj/docfx/**": 1.0,
          "**.md": 2.5,
          "spec/**.md": 3,
          "tutorial/**.md": 1.2
        },
        "keywords": {
          "obj/docfx/**": [ "API", "Reference" ],
          "spec/**.md": [ "Spec", "Conceptual" ],
          "**/*markdown.md": [ "DFM", "Spec" ]
        }
      },
      "dest": "../docs/api",
      "markdownEngineName": "markdig",
      "template": [
        "default",
        "custom_template"
      ]
    }
  }